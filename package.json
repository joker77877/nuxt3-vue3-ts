{
  "name": "mita3d",
  "version": "0.1.0",
  "description": "Element Plus with Nuxt 3 Minimal Starter",
  "author": "Element Plus Team",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "analyze": "nuxi analyze",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .js,.vue,.ts ./ --fix",
    "lint-staged": "lint-staged --no-stash",
    "check": "nuxi typecheck",
    "format": "prettier --write ./",
    "slint": "stylelint \"./**/*.(vue|less|css)\"",
    "slint:fix": "stylelint \"./**/*.(vue|less|css)\" --fix",
    "commit": "git pull && git add . && git cz"
  },
  "dependencies": {
    "@ans1998/vue3-color": "^3.0.7",
    "@ant-design/icons-vue": "^7.0.1",
    "@babylonjs/core": "6.48.0",
    "@babylonjs/loaders": "6.48.0",
    "@babylonjs/materials": "6.48.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^10.9.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.2.1",
    "axios": "^1.6.8",
    "babylonjs-gltf2interface": "6.48.0",
    "dayjs": "^1.11.11",
    "element-plus": "^2.7.1",
    "lodash-es": "^4.17.21",
    "qs": "^6.12.1",
    "spark-md5": "^3.0.2",
    "uuid": "^9.0.1",
    "vue": "^3.4.25",
    "vue-request": "^2.0.4",
    "vue-router": "^4.3.2",
    "vue-wxlogin": "^1.0.4"
  },
  "devDependencies": {
    "@ant-design-vue/nuxt": "^1.4.1",
    "@element-plus/nuxt": "^1.0.9",
    "@iconify/utils": "^2.1.23",
    "@nuxt/image": "^1.6.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/svg-sprite": "^1.0.2",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.15",
    "@types/spark-md5": "^3.0.4",
    "@unocss/nuxt": "^0.58.0",
    "@unocss/transformer-directives": "^0.59.4",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vue/eslint-config-airbnb-with-typescript": "^8.0.0",
    "@vueuse/nuxt": "^10.7.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.2.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.75.0",
    "stylelint": "^16.2.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.0.0",
