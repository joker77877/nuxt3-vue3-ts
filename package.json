    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vue-tsc": "^2.0.17"
  },
  "peerDependencies": {
    "pinia": "^2.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "vite",
        "vue"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
